<?xml version="1.0" encoding="utf-8"?>
<!--  위젯 - 삭제 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	 <!-- default -->
	<item android:state_focused="false" android:state_selected="false"
		android:state_pressed="false" android:drawable="@drawable/del_basic" />
	<item android:state_focused="false" android:state_selected="true"
		android:state_pressed="false" android:drawable="@drawable/del_touch" />
    <!-- Pressed -->
    <item android:state_pressed="true" android:drawable="@drawable/del_touch" />
 	<!-- focused -->
    <item android:state_focused="true"
           android:drawable="@drawable/del_touch" />
</selector> 